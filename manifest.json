{
    "manifest_version": 3,
    "name": "YouTube Video Ad Blocker",
    "version": "1.0",
    "description": "Skip YouTube video ads automatically",
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
  }
  
